    <style type="text/css">
        /*Added by Vishnu For bPopup*/
        #displayMessage {
            background-color: #fff !important;
            border-radius: 15px !important;
            color: #000 !important;
            box-shadow: 0 0 25px 5px #999;
            padding: 20px !important;
            min-width: 200px !important;
            min-height: 100px !important;
        }

        #displayMessageRed {
            background-color: #fff !important;
            border-radius: 15px !important;
            /*color:#000 !important;*/
            box-shadow: 0 0 25px 5px #999;
            padding: 20px !important;
            min-width: 200px !important;
            min-height: 100px !important;
        }

        .button.b-close, .button.bClose {
            color: #fff !important;
            background-color: #64a9d1 !important;
            border-radius: 7px !important;
            box-shadow: none !important;
            font: bold 131% sans-serif !important;
            padding: 0 6px 2px !important;
            position: absolute !important;
            right: -7px !important;
            top: -7px !important;
            cursor: pointer;
        }

        .hide_column {
            display: none;
        }

        .modal1 {
            position: fixed;
            z-index: 999;
            height: 100%;
            width: 100%;
            top: 0;
            background-color: Black;
            filter: alpha(opacity=60);
            opacity: 0.6;
            -moz-opacity: 0.8;
        }

        .center {
            z-index: 1000;
            margin: 300px auto;
            padding: 10px;
            width: 130px;
            background-color: White;
            border-radius: 10px;
            filter: alpha(opacity=100);
            opacity: 1;
            -moz-opacity: 1;
        }

            .center img {
                height: 110px;
                width: 110px;
            }

        .form-control {
            padding: 2px 5px;
        }
        body{
     font-family:'Open Sans' !important;
    }
    </style>
</head>

<body class="infobar-offcanvas sidebar-hideon-collpase sidebar-scroll">


    <header id="topnav" class="navbar navbar-midnightblue navbar-fixed-top clearfix" role="banner">

        <span id="trigger-sidebar" class="toolbar-trigger toolbar-icon-bg">
            <a data-toggle="tooltips" data-placement="right" title="Toggle Sidebar"><span class="icon-bg">
                <i class="fa fa-fw fa-bars"></i></span></a>
        </span>

        <span class="c-text">ITR ECONOMICS</span>
        <ul class="nav navbar-nav toolbar pull-right">
            <li class="dropdown toolbar-icon-bg">
                <a href="#" class="dropdown-toggle" data-toggle='dropdown'><span class="icon-bg"><i class="fa fa-fw fa-user"></i></span></a>
                <ul class="dropdown-menu userinfo arrow">
                    <li><a href="#"><span class="pull-left">Profile</span> <i class="pull-right fa fa-user"></i></a></li>
                    <li class="divider"></li>
                    <li><a href="#"><span class="pull-left">Sign Out</span> <i class="pull-right fa fa-sign-out"></i></a></li>
                </ul>
            </li>
        </ul>
    </header>

    <div id="wrapper">
        <div id="layout-static">
            <div class="static-sidebar-wrapper sidebar-midnightblue">
                <div class="static-sidebar">
                    <div class="sidebar">
                        <div class="widget stay-on-collapse" id="widget-welcomebox">
                            <div class="widget-body welcome-box tabular">
                                <div class="tabular-row">
                                    <img src="~/img/home/logo-header.png" class="img-responsive" />
                                </div>
                            </div>
                        </div>
                        <div class="widget stay-on-collapse" id="widget-sidebar">
                            <nav role="navigation" class="widget-body">
                                <ul class="acc-menu">
                                    <li class="nav-separator">Explore</li>
                                    <li><a href="~/DatacastHome/DashboardWidgets"><i class="fa fa-th-large"></i><span>Dashboard</span></a></li>
                                    <li><a href="~/Home/Dashboard"><i class="fa fa-upload"></i><span>Data Upload</span></a></li>
                                    <li><a href="~/DatacastHome/DataCast"><i class="fa fa-bar-chart-o"></i><span>Analysis</span></a></li>

                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
           
             <div class="static-content-wrapper">
                <div class="static-content">
                    <div class="page-content">
                       
                            @RenderBody()
                     

                        <!--Dispaly Error details/ Alert Message start-->
                        <div id="displayMessage" style="display: none">
                            <!--Add the InnerText at runtime -->
                        </div>
                        <div id="displayMessageRed" style="display: none; color: red;">
                            <!--Add the InnerText at runtime -->
                        </div>
                        <!--Dispaly Error details/ Alert Message end-->


                    </div> <!-- #page-content -->
                </div>
                <footer role="contentinfo">
                    <div class="clearfix">
                        <ul class="list-unstyled list-inline pull-left">
                            <li><h6 style="margin: 0;"> &copy; 2018 DataCast</h6></li>
                        </ul>
                        <button class="pull-right btn btn-link btn-xs hidden-print" id="back-to-top"><i class="fa fa-arrow-up"></i></button>
                    </div>
                </footer>
            </div>
        </div>
    </div>






