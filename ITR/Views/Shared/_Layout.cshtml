            $("#btnBeforeDashboard").click(function (e) {
                e.preventDefault();
                ajaxindicatorstart();
                $("#btnAfterDashboard")[0].click();
            });

            $("#btnBeforeUpload").click(function (e) {
                e.preventDefault();
                ajaxindicatorstart();
                $("#btnAfterUpload")[0].click();
            });


            $("#btnBeforeDatacast").click(function (e) {
                e.preventDefault();
                ajaxindicatorstart();
                $("#btnAfterDatacast")[0].click();
            });


            //Loader start
            function ajaxindicatorstart() {
                if ($('body').find('#loadingDiv').attr('id') != 'loadingDiv') {
                    var loadingString = '<div id="loadingDiv" class="modal1 loaderAssessment"><div id="loading-image" class="center"><img src="../img/home/loader.gif" alt="Loading..." /></div></div>';
                    $('body').append(loadingString);
                    $('#loadingDiv .bg').height('100%');
                    $('#loadingDiv').fadeIn(300);
                    $('body').css('cursor', 'wait');
                }
            };

            function ajaxindicatorstop() {
                $('#loadingDiv .bg').height('100%');
                $('#loadingDiv').remove();
                $('body').css('cursor', 'default');
            }

            $(document).ajaxStart(function () {
                //show ajax indicator 3
                ajaxindicatorstart();
            }).ajaxStop(function () {
                //hide ajax indicator
                ajaxindicatorstop();
            });

            //Loader End



        });
